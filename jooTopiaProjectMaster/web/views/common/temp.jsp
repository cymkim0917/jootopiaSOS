	<script>
	function validate(){
	     //이메일 검사(adfg@dfds.dsf)
        
        //4글자 이상이 나오고 
        //@가 나오고 1글자 이상 주소 . 글자 1~3
        var email = document.getElementById("email").value;
        var regExp = /\w{4,}@\w{1,}\.\w{1,3}/
        if(regExp.test(email)){
       	 alert('이메일 입력 완료');
        }else{
       	 alert('이메일 잘못 입력');
       	document.getElementById("email").select();
       	 return false;
        }
       
        
          
    	  //아이디 검사
        //첫글자는 반드시 영문 소문자, 총 4~12자로 이루어지고
        //숫자가 반드시 하나 이상 포함되어야 함
        //영문 소문자와 숫자로 이루어져야 한다.
        
        var userId = document.getElementById("userid").value;
        //var regExp2 = /[a-z]([a-z]|[0-9]{1,}){3,12}/;
        var regExp2 = /[a-z]([a-z0-9]{1,}){3,12}/;
        var regExp3 = /[a-z][0-9]+/;
        
        if(!(regExp2.test(userId) && regExp3.test(userId))){
           alert("아이디를 잘못 입력하셨습니다.")
           document.getElementById("userid").select();
           return false;
        }else{
           alert("아이디를 정상 입력했습니다.");
        }
   
       
        //비밀번호 확인 검사
        //비밀번호와 비밀번호 확인 값이 일치
        var pass= document.getElementById("pass").value;
        var pass1= document.getElementById("pass").value;
        
        if(pass!=pass1){
       	 alert("비밀번호  잘못 입력");
       	document.getElementById("pass").select();
        }
        
     
        //이름 검사
        //2글자 이상, 한글만
        var name = document.getElementById("name").value;
        var regExp3 = /[가-힣]{2,}/;
        var regExp33 = /[\{\}\[\]\/?.,;:|\)*~`!^\-_+<>@\#$%&\\\=\(\'\"]/gi;
       // var regExpName = /[a-z0-9A-Z]|[ \[\]{}()<>?|`~!@#$%^&*-_+=,.;:\"\'\\]/g;
        var regExp32 = /[0-9]/;

        
        
        if(!regExp3.test(name) || regExp33.test(name) || regExp32.test(name)){
       	 alert('이름 잘못 입력');
       	 document.getElementById("name").select();
       	 return false;
        }else{
       	 alert('이름 정상 입력')
        }
       
         
     
        //전화번호 검사
        //전화번호 앞자리는 2~3자리 숫자
        //두번째자리는 3~4자리 숫자
        //세번째 자리는 4자리 숫자
        var regExp4 = /[0-9]{2,3}/;
        var regExp5 = /[0-9]{3,4}/;
        var regExp6 = /[0-9]{4}/;
        
        var tel1 = document.getElementById("tel1").value;
        var tel2 = document.getElementById("tel2").value;
        var tel3 = document.getElementById("tel3").value;
        
        if(!(regExp4.test(tel1)&&regExp5.test(tel2)&&regExp6.test(tel3))){
       		alert("전화번호 잘못 입력");
       		return false;
       		document.getElementById("tel1").select();
        }else{
        	alert("전화번호 정상 입력");
        }
        	return true;
	}
	
	</script>

